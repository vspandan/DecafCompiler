	a[2]=30;
	a[3]=40;
	a[4]=25;
	a[5]=85;
	a[6]=75;
	 search = 85;       


   last = n - 1;
   middle = (first+last)/2;
 
   for first =0, (last+1 )
   {
      	if ( a[middle] < search ){
         	first = middle + 1;    
	}

	if ( a[middle] == search ) 
	{
		callout ("printf","%d found at location %d.\n", search, middle+1);
	 	break;
      	}
     	if(a[middle] > search){
